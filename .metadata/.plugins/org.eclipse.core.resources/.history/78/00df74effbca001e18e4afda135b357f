
public class StringTest {

	public static void main(String[] args) {
		// 문자열 자료형 이용하기
		String name="유병승";
		System.out.println(name);
		
		String name2=new String("김동훈");
		System.out.println(name2);
		
		
		// + 연산자이용을 하면 문자열 결합연산을 실행함.
		String msg;
		msg="자바 선생님 "+"유병승";
		System.out.println(msg);
		msg="학생 "+name2;
		System.out.println(msg);
		

	}

}
